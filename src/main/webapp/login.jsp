        return this.pub_key; 
    },

    setTarget:function(target) {
        $("#reqFrm").attr("target", target);
    },

    setAction:function(url) {
        $("#reqFrm").attr("action", url);
    },

    setValue:function(name, value) {
        $("#reqFrm").find("input[name="+name+"]").val(value);
    },

    getValue:function(name) {
        return $("#reqFrm").find("input[name="+name+"]").val();
    },

    setLoginMethod:function(method) {
        this.setValue("login_method", method);
    },

    setUserId:function(id) {
        var val = RSA.encrypt(encodeURIComponent(id), this.getPubKey());
        this.setValue("userid", val);
    },

    setPasswd:function(passwd) {
        var val = RSA.encrypt(encodeURIComponent(passwd), this.getPubKey());
        this.setValue("password", val);
    },

    setChkAutoLogin:function(v) {
        this.setValue("chk_auto_login", v);
    },

    setChkSecure:function(v) {
        this.setValue("chk_secure", v);
    },

    setReturnUrl:function(url) {
