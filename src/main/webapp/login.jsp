        this.setValue("captcha_key", v);
    },

    setCaptchaValue:function(v) {
        this.setValue("captcha_value", v);
    },
    setSnsConnection:function(v) {
        this.setValue("snsConnection", v);
    },
    setSnsLinked:function(v) {
        this.setValue("snsLinked", v);
    },
    setSnsActionType:function(v) {
        this.setValue("snsActionType", v);
    },

    submit:function() {
        var login_url = "";
        if (location.protocol == 'https:')
            this.setValue("cb_proto", "https");
        else
            this.setValue("cb_proto", "http");

        this.setValue("cb_domain", location.hostname);
        this.setValue("loginFrom", "");
        this.setValue("returnTarget", "");

        if (this.getValue("chk_secure") == "secure")
            login_url = "https://login.ticketmonster.co.kr";
        else
            login_url = "http://www.ticketmonster.co.kr";

        this.setAction(login_url + "/user/login");

        $("#reqFrm").submit();
    }
};

</script>

